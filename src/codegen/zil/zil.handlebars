{{#each map.rooms}} 
<ROOM {{zilName this.name}}
  (LOC ROOMS)
  (DESC "{{this.name}}")
  {{#each this.connections}}
  ({{upperDirToStr this.startDir this.startType }} TO {{zilName this.room.name}}){{#if this.name}} ; {{{this.name}}}{{/if}}
  {{/each}}
  (FLAGS {{#unless this.dark}}ONBIT{{/unless}})
  >

{{#each this.objects}}  
{{> zilObject object=this container=../this}}
{{/each}}

{{/each}}